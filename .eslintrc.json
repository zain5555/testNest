{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "env": {
    "es2020": true,
    "node": true
  },
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "error"
